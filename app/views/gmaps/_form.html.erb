<%= simple_form_for(@gmap) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :latitude  %>
    <%= f.input :longitude  %>
    <%= f.input :address  %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
