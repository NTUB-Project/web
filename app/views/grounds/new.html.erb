<h1>新增場地廠商</h1>

<%= form_for(@ground) do |f| %>
    <%= f.label :name, "名稱" %>
    <%= f.text_field :name %> <br />
    
    <%= f.label :introduction, "簡介" %>
    <%= f.text_area :introduction %> <br />
    
    <%= f.label :location, "地址" %>
    <%= f.text_field :location %> <br />
    
    <%= f.submit %>
<% end %>
